  "dependencies": {
    "@chakra-ui/icons": "^2.0.14",
    "@chakra-ui/react": "^2.4.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@next/font": "13.0.7",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "date-fns": "^2.29.3",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "framer-motion": "^6.5.1",
    "next": "13.0.7",
    "next-i18next": "^13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.2.0",
    "@markuplint/react-spec": "^3.2.0",
    "@netlify/plugin-nextjs": "^4.37.4",
    "http-server": "^14.1.1",
    "markuplint": "^3.2.0",
    "netlify-cli": "^15.4.1",
    "next-sitemap": "^3.1.43",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1"
  }
