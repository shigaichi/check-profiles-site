  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "export": "next export",
    "postexport": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
